{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-console": [
      false,
      "log",
      "error"
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "object-literal-key-quotes": false,
    "jsx-wrap-multiline": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
